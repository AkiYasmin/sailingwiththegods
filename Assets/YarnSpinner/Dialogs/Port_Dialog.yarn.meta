    COMMAND_TEXT \"setspeaker DialogScreen other \"\n130:33 COMMAND_TEXT_END \">>\"\n131:0
    TEXT \"I don't believe you. \"\n131:21 TEXT_EXPRESSION_START \"{\"\n131:22 VAR_ID
